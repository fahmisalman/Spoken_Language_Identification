    "    length.append(samplerate)\n",
    "    \n",
    "label = []\n",
    "for filename in os.listdir(train_path):\n",
    "    label.append(filename[:2])\n",
    "    \n",
    "data = {'series': series,\n",
    "       'languange':label}\n",
    "\n",
    "df = pd.DataFrame(data)\n",
