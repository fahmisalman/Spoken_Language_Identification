    "    length.append(samplerate)\n",
    "    \n",
    "label = []\n",
    "for filename in os.listdir(test_path):\n",
    "    label.append(filename[:2])\n",
    "    \n",
    "data_test = {'series': series,\n",
    "       'languange':label}\n",
    "\n",
    "df_test = pd.DataFrame(data_test)\n",
    "df_test.head()"
   ]
